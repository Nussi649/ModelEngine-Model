<Classes>
    <Class name="PayAcc">
        <Attribute type="text" required="true" is_key="true">Name</Attribute>
        <Reference type="Tx" multiplicity="multi" required="false">Transactions</Reference>
        <Reference type="TimeSeriesPoint" multiplicity="multi" required="false">Balances</Reference>
    </Class>
    <Class name="InvAcc">
        <Attribute type="text" required="true" is_key="true">Name</Attribute>
        <Reference type="Tx" multiplicity="multi" required="false">Transactions</Reference>
        <Reference type="TimeSeriesPoint" multiplicity="multi" required="false">Sums</Reference>
    </Class>
    <Class name="Tx">
        <Attribute type="text" required="true" is_key="true">UUID</Attribute>
        <Attribute type="float" required="true">Amount</Attribute>
        <Attribute type="text" required="true">Description</Attribute>
        <Attribute type="datetime" required="true">Timestamp</Attribute>
    </Class>
    <Class name="TimeSeriesPoint">
        <Attribute type="text" required="true" is_key="true">UUID</Attribute>
        <Attribute type="float" required="true">Value</Attribute>
        <Attribute type="datetime" required="true">Timestamp</Attribute>
    </Class>
</Classes>