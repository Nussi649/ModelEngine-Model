<Classes>
    <Class name="AssetAccount">
        <Collection>Account</Collection>
        <Attribute type="text" is_key="true" required="true">name</Attribute>
        <Reference type="Tx" multiplicity="multi" required="false">transactions</Reference>
    </Class>
    <Class name="BudgetAccount">
        <Collection>Account</Collection>
        <Attribute type="text" is_key="true" required="true">name</Attribute>
        <Attribute type="float" required="false">yearly_budget</Attribute>
        <Reference type="Tx" multiplicity="multi" required="false">transactions</Reference>
    </Class>
    <Class name="Tx">
        <Collection>Base</Collection>
        <Attribute type="text" is_key="true" required="true">identifier</Attribute>
        <Attribute type="text" required="true">description</Attribute>
        <Attribute type="float" required="true">amount</Attribute>
        <Attribute type="datetime" required="true">time</Attribute>
    </Class>
</Classes>